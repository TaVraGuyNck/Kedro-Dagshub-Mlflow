{
  "name": "Kedro + MLflow (Python 3.9.13)",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "customizations": {
    "vscode": {
      "settings": {
        "python.defaultInterpreterPath": "/root/.pyenv/versions/3.9.13/bin/python"
      },
      "extensions": [
        "ms-python.python",
        "ms-toolsai.jupyter"
      ]
    }
  },
  "postCreateCommand": "pip install -e . && pip install python-dotenv && python -m ipykernel install --user --name python3.9.13",
  "workspaceFolder": "/workspaces/${localWorkspaceFolderBasename}"
}